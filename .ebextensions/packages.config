packages:
  yum:
    postgresql-devel: []
    jq: []
commands:
  setvars:
    command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
container_commands:
  migration:
    leader_only: true
    command: "source /var/app/venv/*/bin/activate : pipenv run python manage.py migrate 1> /var/thing1.txt 2> /var/thing2.txt"
